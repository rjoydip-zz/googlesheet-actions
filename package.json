{
  "name": "googlesheet-actions",
  "version": "0.1.1",
  "description": "Github action for google spreadsheets to fetch data",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint:fix && npm test && npm run pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rdipp/googlesheet-actions.git"
  },
  "keywords": [
    "actions",
    "googlesheet"
  ],
  "author": "rjoydip",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rdipp/googlesheet-actions/issues"
  },
  "homepage": "https://github.com/rdipp/googlesheet-actions#readme",
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@actions/github": "^4.0.0",
    "axios": "^0.21.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.7.12",
    "@typescript-eslint/parser": "^2.8.0",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "js-yaml": "^3.13.1",
    "nock": "^13.0.2",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.6.4"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run all",
      "pre-commit": "npm run all"
    }
  }
}
